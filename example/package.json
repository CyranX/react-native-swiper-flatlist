{
  "name": "SwiperFlatListExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android && adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios",
    "postinstall": "rm -rf node_modules/react-native-swiper-flatlist/node_modules/react-native",
    "watch": "node ./scripts/watch-and-copy.js",
    "start:watch": "concurrently 'npm run watch' 'npm run start'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "detox:build": "detox build --configuration ios.sim.debug",
    "detox:test": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "detox": "^9.0.5",
    "react": "16.6.0-alpha.8af6728",
    "react-native": "0.57.4",
    "react-native-swiper-flatlist": "*"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "concurrently": "^2.2.0",
    "fs-extra": "^0.30.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.0",
    "minimatch": "^3.0.2",
    "node-watch": "^0.4.0",
    "react-test-renderer": "16.5.0",
    "rimraf": "^2.5.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/SwiperFlatListExample.app",
        "build": "xcodebuild -project ios/SwiperFlatListExample.xcodeproj -scheme SwiperFlatListExample -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  }
}
